# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Aces' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Aces

pod 'Firebase'
pod 'Firebase/Core'
pod 'Firebase/Messaging'
pod 'Firebase/Auth'
pod 'GoogleSignIn'
pod 'GoogleMaps'
pod 'GooglePlaces'
pod 'Toast-Swift'
pod 'Firebase/Database'
pod 'SearchTextField'
pod 'Firebase/RemoteConfig'

  target 'AcesTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AcesUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
